Private Sub Command1_Click()
Dim loopc As Integer
For loopc = 0 To 9
    Call WriteVar(App.Path & "\Data\INIT\Macro.tsao", "Macro", "Tecla" & loopc, Text1(loopc).text)
Next loopc
Unload Me
End Sub

Private Sub Command2_Click()
