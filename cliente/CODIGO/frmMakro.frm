End Sub

Private Sub Form_Load()
Dim loopc As Integer
For loopc = 0 To 9
    Text1(loopc).text = GetVar(App.Path & "\Data\INIT\Macro.tsao", "Macro", "Tecla" & loopc)
Next loopc
End Sub

Private Sub Option1_Click()
